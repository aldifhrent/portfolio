generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model WorkExperience {
  id          String   @id @default(uuid())
  workId      String   @unique
  workPlace   String
  role        String
  startDate   String
  endDate     String
  description String[] @default([])

  @@map("workexperience")
}
